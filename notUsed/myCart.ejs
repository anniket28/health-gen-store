    <section class="mycart">
      <h1>My Cart</h1>
      <% if(product) { %>
        <div class="container-fluid">
          <div class="row">
            <div class="col"><strong>Product Name</strong></div>
            <div class="col"><strong>Product Quantity</strong></div>
            <div class="col"><strong>Total Product Price</strong></div>
            <div class="col"><strong></strong></div>
          </div>
          </div>
          <div class="container-fluid">
            <% product.forEach(function(products) { %>
              <div class="row">
                <div class="col"><%= products.item.name %></div>
                <div class="col"><%= products.qty %></div>
                <div class="col"><%= products.price %></div>
                <div class="col"><a href="#" class="btn btn-primary"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash-fill" viewBox="0 0 16 16">
                <path d="M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10a1 1 0 0 0-1-1H7a1 1 0 0 0-1 1H2.5zm3 4a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 .5-.5zM8 5a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7A.5.5 0 0 1 8 5zm3 .5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 1 0z"/>
              </svg><i class="bi bi-trash-fill"></i> Remove</a></div>
            </div>
            <% }) %>
          </div>
          <div class="container-fluid">
            <div class="row">
              <div class="col total"><strong>Cart Total : Rs. <%= totalPrice %></strong></div>
            </div>
          </div>
        <% } else { %>
          <hr>
        <p class="empty">Your Cart is Empty! 
          <br>
          <a href="/products">Go to products page to Add something</a></p>
      <% } %>
    </section>


