  <section class="settings">
    <div class="card">
    <h1 class="card-title">Change Password</h1>
      <form action="/change" method="post">
        <div class="form-group form">
          <% if(successp.length>0){ %>
            <div class="alert-success alert alert-dismissible fade show" role="alert" style="font-size: 20px;">
                <%= successp %>
                <button type="submit" class="close" data-dismiss="alert" aria-label="close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        <% } %>
          <label for="old"><span class="pass">Old Password :</span></label>
          <input type="password" class="form-control" id="old" name="oldPassword" placeholder="Enter your old password here." required oninvalid="this.setCustomValidity('Old Password is required')" oninput="this.setCustomValidity('')">
          </div>
          <% if(erroro.length>0){ %>
            <div class="alert-danger alert alert-dismissible fade show" role="alert" style="font-size: 20px;">
                <%= erroro %>
                <button type="submit" class="close" data-dismiss="alert" aria-label="close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        <% } %>
        <div class="form-group">
          <label for="new"><span class="pass">New Password :</span></label>
          <input type="password" class="form-control" id="new" name="newPassword" placeholder="Enter your new password here." required oninvalid="this.setCustomValidity('New Password is required')" oninput="this.setCustomValidity('')">
          </div>
        <div class="form-group">
          <label for="retype"><span class="pass">Confirm Password :</span></label>
          <input type="password" class="form-control" id="retype" name="confirmPassword" placeholder="Retype your new password here." required oninvalid="this.setCustomValidity('Confirm Password is required')" oninput="this.setCustomValidity('')">
          </div>
          <% if(errorn.length>0){ %>
            <div class="alert-danger alert alert-dismissible fade show" role="alert" style="font-size: 18px;">
                <%= errorn %>
                <button type="submit" class="close" data-dismiss="alert" aria-label="close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        <% } %>
          <button class="btn btn-primary btns">Submit</button>
      </form>
      </div>
