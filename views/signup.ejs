      <section class="login">
        <div class="card cardsu">
            <div class="card-body">
              <h1 class="card-title">Sign Up</h1>
              <p class="card-text">
                
                <form action="userRegister" method="post">
                    <div class="form-group">
                      <label for="fname"><span class="fname">First Name :</span></label>
                      <input type="text" class="form-control" id="fname" name="fname" placeholder="Enter your first name" required oninvalid="this.setCustomValidity('First Name is required.')" oninput="this.setCustomValidity('')">
                    </div>
                    <div class="form-group">
                      <label for="lname"><span class="lname">Last Name :</span></label>
                      <input type="text" class="form-control" id="lname" name="lname" placeholder="Enter your last name" required oninvalid="this.setCustomValidity('Last Name is required.')" oninput="this.setCustomValidity('')">
                    </div>
                    <div class="form-group">
                      <label for="mail"><span class="mail">Email :</span></label>
                      <input type="email" class="form-control" id="mail" name="email" placeholder="Enter your email id" required pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,}$" inputmode="email" oninvalid="this.setCustomValidity('Email is either empty or invalid.')" oninput="this.setCustomValidity('')">
                      <% if(errore.length>0){ %>
                        <div class="alert-danger alert alert-dismissible fade show" role="alert" style="margin-top: 5px !important; font-size: 20px !important;">
                            <%= errore %>
                            <button type="submit" class="close" data-dismiss="alert" aria-label="close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                    <% } %>
                    </div>
                    <div class="form-group">
                      <label for="pass"><span class="pass">Password :</span></label>
                      <input type="password" class="form-control" id="pass" name="password" placeholder="Enter a password" required minlength="8" inputmode="tel" oninvalid="this.setCustomValidity('Password must be equal to or greater than 8 characters.')" oninput="this.setCustomValidity('')">
                    </div>
                    <div class="form-group">
                      <label for="cont"><span class="cont">Contact Number :</span></label>
                      <input type="tel" class="form-control" id="cont" name="contact" placeholder="Enter your contact number" required pattern="[0-9]{3}[0-9]{3}[0-9]{4}" oninvalid="this.setCustomValidity('Contact Number is either empty or invalid.')" oninput="this.setCustomValidity('')">
                    </div>
                    <div class="form-group">
                      <label for="city"><span class="city">City :</span></label>
                      <input type="text" class="form-control" id="city" name="city" placeholder="Enter your city" required oninvalid="this.setCustomValidity('City is required.')" oninput="this.setCustomValidity('')">
                      <div id="ccity" class="emsg"></div>
                    </div>
                    <div class="form-group">
                      <label for="zcode"><span class="zcode">Pincode :</span></label>
                      <input type="tel" class="form-control" id="zcode" name="pcode" placeholder="Enter your area pincode" inputmode="numeric" pattern="[0-9]{3}[0-9]{3}" oninvalid="this.setCustomValidity('Pincode is either empty or invalid.')" oninput="this.setCustomValidity('')" required>
                    </div>
                    <button type="submit" class="btn btn-primary btns st">Submit</button>
                    <button type="reset" class="btn btn-primary btns rst">Reset</button>
                </form> 
                <p class="acc" style="font-size: 18px !important;">Already Have an Account.<a class="card-link" href="/login"> Login</a></p>
              </p>
